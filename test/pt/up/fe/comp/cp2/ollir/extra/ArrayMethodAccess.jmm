
import ioPlus;

class ArrayMethodAccess {

    public int index() {
        return 2;
    }

    public static void main(String[] args) {
        int[] nums;
        int i;
        ArrayMethodAccess obj;
        obj = new ArrayMethodAccess();

        nums = new int[4];
        nums[0] = 10;
        nums[1] = 20;
        nums[2] = 30;
        nums[3] = 40;

        i = 1;

        ioPlus.printResult(nums[i]);
        ioPlus.printResult(nums[obj.index()]);
        ioPlus.printResult(nums[nums[0] / 10]);
        ioPlus.printResult(nums[nums.length - 2]);
    }
}