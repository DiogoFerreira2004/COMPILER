class ComplexWhile {
    public int countEven(int[] numbers) {
        int count = 0;
        int i = 0;

        while (i < numbers.length) {
            if ((numbers[i] / 2) * 2 == numbers[i]) {
                count = count + 1;
            }
            i = i + 1;
        }

        return count;
    }
}