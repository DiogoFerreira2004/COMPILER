        c = a * b + (a - b) / 2;


        c = (a + b) * (a - b);

        c = ((a + b) * 2) - (a / (b + 1));

        c = this.arithmeticMethod(a, b) + this.arithmeticMethod(b, a);

        return c;
    }

    public boolean booleanMethod(boolean a, boolean b) {
        boolean c;

        c = a && b || (!a && !b);

        c = (a && b) || (!a && !b);

        c = ((a && b) || (!a)) && ((a || b) && (!b));

